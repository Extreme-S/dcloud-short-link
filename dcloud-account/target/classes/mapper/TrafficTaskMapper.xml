<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.mapper.TrafficTaskMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="org.example.model.TrafficTaskDO">
        <id column="id" property="id" />
        <result column="account_no" property="accountNo" />
        <result column="traffic_id" property="trafficId" />
        <result column="use_times" property="useTimes" />
        <result column="lock_state" property="lockState" />
        <result column="message_id" property="messageId" />
        <result column="gmt_create" property="gmtCreate" />
        <result column="gmt_modified" property="gmtModified" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, account_no, traffic_id, use_times, lock_state, message_id, gmt_create, gmt_modified
    </sql>

</mapper>
